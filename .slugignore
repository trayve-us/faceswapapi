__pycache__/
*.pyc
*.pyo
*.pyd
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg

# Exclude test files
test/
tests/
*test*
spec/

# Exclude docs and assets (but keep requirements.txt!)
docs/
*.md
*.rst
LICENSE
README*

# Exclude development files
.git/
.gitignore
.vscode/
.idea/
*.log

# Exclude any models or weights
*.pth
*.pt
*.bin
weights/
models/

# Exclude other deployment configs
Dockerfile
.dockerignore
railway.yml
download_models.sh
main_new.py
main_old.py
